{
  "name": "ink",
  "rules": {
    "ink": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "_toplevel"
      }
    },
    "_toplevel": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "text"
        },
        {
          "type": "SYMBOL",
          "name": "tag"
        },
        {
          "type": "SYMBOL",
          "name": "choice"
        },
        {
          "type": "SYMBOL",
          "name": "knot"
        },
        {
          "type": "SYMBOL",
          "name": "todo_comment"
        }
      ]
    },
    "text": {
      "type": "PATTERN",
      "value": "[^\\n\\/#*\\[\\]=]+?"
    },
    "tag": {
      "type": "PATTERN",
      "value": "#[^\\n#]+"
    },
    "choice": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "mark",
          "content": {
            "type": "SYMBOL",
            "name": "symbol"
          }
        },
        {
          "type": "FIELD",
          "name": "text",
          "content": {
            "type": "SYMBOL",
            "name": "choice_text"
          }
        }
      ]
    },
    "choice_text": {
      "type": "CHOICE",
      "members": [
        {
          "type": "FIELD",
          "name": "main",
          "content": {
            "type": "SYMBOL",
            "name": "text"
          }
        },
        {
          "type": "SYMBOL",
          "name": "_compound_choice_text"
        }
      ]
    },
    "_compound_choice_text": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "FIELD",
            "name": "main",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "text"
                },
                {
                  "type": "BLANK"
                }
              ]
            }
          },
          {
            "type": "SYMBOL",
            "name": "bracket_open"
          },
          {
            "type": "FIELD",
            "name": "temporary",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "text"
                },
                {
                  "type": "BLANK"
                }
              ]
            }
          },
          {
            "type": "SYMBOL",
            "name": "bracket_close"
          },
          {
            "type": "FIELD",
            "name": "final",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "text"
                },
                {
                  "type": "BLANK"
                }
              ]
            }
          }
        ]
      }
    },
    "knot": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "knot_mark"
          },
          {
            "type": "FIELD",
            "name": "name",
            "content": {
              "type": "SYMBOL",
              "name": "identifier"
            }
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SEQ",
                "members": [
                  {
                    "type": "STRING",
                    "value": " "
                  },
                  {
                    "type": "SYMBOL",
                    "name": "knot_mark"
                  }
                ]
              },
              {
                "type": "BLANK"
              }
            ]
          }
        ]
      }
    },
    "knot_mark": {
      "type": "PATTERN",
      "value": "==+"
    },
    "identifier": {
      "type": "PREC",
      "value": 1,
      "content": {
        "type": "PATTERN",
        "value": "[a-zA-z_][a-zA-Z0-9_]*"
      }
    },
    "symbol": {
      "type": "STRING",
      "value": "*"
    },
    "bracket_open": {
      "type": "STRING",
      "value": "["
    },
    "bracket_close": {
      "type": "STRING",
      "value": "]"
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "PATTERN",
            "value": "\\/\\/[^\\n]*\\n"
          },
          {
            "type": "PATTERN",
            "value": "\\/\\*(.|\\r?\\n)+\\*\\/"
          }
        ]
      }
    },
    "todo_keyword": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": 1,
        "content": {
          "type": "STRING",
          "value": "TODO"
        }
      }
    },
    "todo_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "todo_keyword"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "PATTERN",
          "value": "[^\\n]*"
        },
        {
          "type": "PATTERN",
          "value": "\\n"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
